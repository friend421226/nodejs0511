<%- include('./includes/header') %>
</head>
<body>
    <img src="/images/Pinkoi_logo.jpeg" alt="">
    <h1><%= pageTitle %> :)</h1>
    <% if (products.length > 0) { %>
        <% products.forEach((product) => { %>
            <img src="<%= product.imageUrl %>" alt="<%= product.title %>">
            <h3><%= product.title %></h3>
        <% }) %>
    <% } else { %>
        尚無商品
    <% } %>
</body>
</html>